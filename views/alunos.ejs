<!DOCTYPE html>
<html lang="pt-BR">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Lista de Alunos</title>
</head>
<body>
    <h1>Lista de Alunos</h1>
    <table border="1">
        <thead>
            <tr>
                <th>Nome</th>
                <th>Data de Nascimento</th>
                <th>Matr√≠cula</th>
                <th>Curso</th>
            </tr>
        </thead>
        <tbody>
            <% alunos.forEach(aluno => { %>
                <tr>
                    <td><%= aluno.nome %></td>
                    <td><%= aluno.dataNascimento.toLocaleDateString() %></td>
                    <td><%= aluno.matricula %></td>
                    <td><%= aluno.curso %></td>
                </tr>
            <% }) %>
        </tbody>
    </table>
</body>
</html>
